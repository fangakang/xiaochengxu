@import "../../icon/iconfont.wxss";
page{
  background: #f6f6f6;
}
.content{
  padding:0 30rpx;
  overflow: hidden;
  margin-top: 20rpx;
}
.color999{
  color:#999;
}
.color222{
  color:#222;
}
.color777{
  color:#777;
}
.colorR{
  color:#f24c36;
}
.content>view{
  margin-bottom: 20rpx;
}
.f26{
  font-size: 26rpx;
}
.themeContent{
  overflow: hidden;
  width: 750rpx;
}
.giftCon{
  float:left;
  width: 310rpx;
  color:#aaa;
  background: #fff;
  border-radius: 15rpx;
  text-align: center;
  font-size: 24rpx;
  padding:2rpx 2rpx 16rpx 2rpx;
  margin-bottom: 30rpx;
  margin-left:38rpx;
  border: 2rpx solid #fff;
}
.giftCon image{
  border-radius: 10rpx;
  width:310rpx;
  height:190rpx;
}
.content{
  padding: 0px
}
.spellGroup{
  background: #fff;
  padding-top: 45rpx;
  padding-bottom: 30rpx;
}
.chiseseTitle{
  text-align: center
}
.chiseseTitle image{
  width: 198rpx;
  height:2rpx;
  display: inline-block
}
.chiseseTitle view{
  display: inline-block;
  margin:0 32rpx;
  position: relative;
  top:10rpx;
}
.englishTitle{
  text-align: center;
  color:#ccc;
  margin-top:20rpx;
  margin-bottom: 34rpx;
}
.imgContent .groupImg{
  width: 100%;
  height:400rpx;
}
.groupFooter{
  font-size: 30rpx;
  padding-left:24rpx;
  overflow: hidden;
  margin-top:25rpx;
}
.groupFooter view{
  float:left;
  margin-top: 5rpx;
}
.groupFooter button{
  float: right;
  background: #f24c36;
  color: #fff;
  font-size:26rpx;
  line-height:60rpx;
  width:180rpx;
  margin-right: 25rpx;
}
.imgContent{
  position: relative
}
.groupType{
  position:absolute;
  right:0;
  top:30rpx;
  padding:0 25rpx;
  font-size: 24rpx;
  color:#fff;
  line-height:50rpx;
  border-bottom-left-radius: 25rpx;
  border-top-left-radius: 25rpx;
  background: rgba(242,76,54,.8);
}
.activityDetail{
  padding:24rpx;
  color:#fff;
  width: 100%;
  background: -webkit-linear-gradient(left, rgba(242,76,54,.8) , rgba(198,44,70,.8)); /* Safari 5.1 - 6.0 */
  background: -o-linear-gradient(right, rgba(242,76,54,.8), rgba(198,44,70,.8)); /* Opera 11.1 - 12.0 */
  background: -moz-linear-gradient(right, rgba(242,76,54,.8), rgba(198,44,70,.8)); /* Firefox 3.6 - 15 */
  background: linear-gradient(to right, rgba(242,76,54,.8) , rgba(198,44,70,.8)); /* 标准的语法（必须放在最后） */
  overflow: hidden;
  position:absolute;
  bottom:9rpx;
}
.groupPrice{
  float: left;
}
.endTime{
  float: right;
  margin-right: 48rpx;
  margin-top: 10rpx;
}
.price view{
  display: inline-block
}
.f28{
  font-size: 28rpx;
}
.f24{
  font-size: 24rpx;
}
.f30{
  font-size: 30rpx;
}
.original{
  text-decoration:line-through;
  margin-left: 24rpx;
}
.bargaining,.secondKill{
  background: #f6f6f6;
  padding-top: 45rpx;
  padding-bottom: 30rpx;
}
.secondContent{
  padding-left:26rpx;
  overflow: hidden;
}
.product{

  float: left;
  position: relative;
}
.product image{
  width: 272rpx;
  height:230rpx;
}
.participate{
  position:absolute;
  left:0;
  top:20rpx;
  padding:0 25rpx 0 5rpx;
  font-size: 24rpx;
  color:#fff;
  line-height:50rpx;
  border-bottom-right-radius: 25rpx;
  border-top-right-radius: 25rpx;
  background: rgba(242,76,54,.8);
}
.productDetail{
  float:left;
  margin-left: 20rpx;
}
.lowPrice view{
  display: inline-block
}
.remain{
  margin:10rpx 0;
}
.btnCon{
  margin-top: 20rpx;
}
.btnCon button{
  display: inline-block;
  width: 160rpx;
  line-height: 60rpx;
  font-size: 26rpx;
  color:#fff;
  background: #f24c36;
  margin-right: 25rpx;
}
.btnCon view{
  color:#f24c36;
  float:right;
  margin-top: 15rpx;
}
.themeChoose{
  overflow: hidden;
}
.groupFooter .notStart,.btnCon .notStart{
  background: #bbb;
}
.themtTitle{
  width: 310rpx;
  text-align: center;
   display:-webkit-box;
  -webkit-box-orient:vertical;
  overflow:hidden;
  margin-top: 8rpx;
  height:35rpx;
}
.footer{
  position: fixed;
  bottom:0;
  left:0;
  right:0;
  display: flex;
  background: #fff;
  padding:20rpx 0rpx;
  border-top:2rpx solid #f3f3f3;
}
.footer>navigator,.footer>view{
  flex:1;
  text-align: center;
  color:#3c3c3c;
  font-size: 26rpx;
}
.footer .iconfont{
  font-size:40rpx;
}
.footer .active{
  color:#f24c36;
}
.fixedBtn{
  width:92rpx;
  height:92rpx;
  border-radius: 50%;
  background: rgba(252,215,45,.8);
  border: 2rpx solid #fff;
  color:#382903;
  position: fixed;
  text-align: center;
  font-size: 20rpx;
  right:30rpx;
  padding: 0;
}
.fixedBtn::after{
  border: none;
}
.fixedBtn .iconfont{
  display:inline-block;
  margin-top:-5px;
}
.word{
  position:relative;
  bottom:35rpx;
  
}
.share{
  bottom:210rpx;
}
.introduce,.service{
  bottom:100rpx;
}
.use{
  bottom:54rpx;
  right:312rpx;
  width:110rpx;
  height:110rpx;
  background: #fff;
  border:1rpx solid #f24c36;
  color:#f24c36;
}
.use .iconfont{
  margin-top: 5rpx;
  color:#f24c36;
}
.introduceMask{
  position: fixed;
  top:0;
  left:0;
  bottom: 0;
  right:0;
  background: rgba(0,0,0,.5)
}
.maskContent{
  background: #fff;
  width: 600rpx;
  border-radius:15rpx;
  padding: 35rpx 0;
  position: absolute;
  left:75rpx;
  top:160rpx;
}
.maskTitle{
  text-align: center;
  position: relative;
}
.maskTitle image{
  width:332rpx;
  height:75rpx;
  margin: 0 auto;
  margin-bottom: 20rpx;
}
.maskTitle span{
  position: absolute;
  font-size: 36rpx;
  color:#fff;
  top:8rpx;
  left:228rpx;
  z-index: 100
}
.maskBody{
  padding:0 55rpx;
  font-size: 26rpx;
  color:#666;
  line-height: 42rpx;
  margin-bottom: 50rpx;
  height:504rpx;
  overflow-y: scroll;
}
.indent{
  text-indent:52rpx
}
.maskFooter{
  text-align: center;
}
.maskFooter button{
  display: inline-block;
  margin: 0 auto;
  background: #f24c36;
  color:#fff;
  font-size: 28rpx;
  border-radius: 38rpx;
  width:200rpx;
}
.maskFooter button::after{
  border:none;
}
.unScroll{
  position: fixed;
  top:0;
  bottom:0;
  left:0;
  right:0;
  overflow: hidden;
}
.loadAll{
  display: flex;
  height:50rpx;
  margin-top: 20rpx;
}
.loadAll>view{
  flex:1;
  color:#bbb;
  font-size: 28rpx;
  text-align: center
}
.line image{
  height:2rpx;
  width:150rpx;
  margin-top: 22rpx;
}
.line .fl{
  float: left
}
.line .fr{
  float: right
}
.noGiftCard{
  text-align: center;
  font-size: 30rpx;
  color: #bbb;
  margin-top: 20rpx;
}
.noGiftCard image{
  margin: 0 auto;
  width:350rpx;
  height:200rpx;
}
