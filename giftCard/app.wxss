/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding:0;
  box-sizing: border-box;
} 
page{
  background: #efefef
}
/**app.wxss**/

view{
  box-sizing: border-box;
  font-family: Microsoft YaHei,Arial, Helvetica,"\534E\6587\9ED1\4F53", sans-serif;
}
page{
  background: #efefef;
}
view.flex_box{
  display: flex;
  display:-webkit-flex;
}
view.flex_middle{
  align-items: center;
}
view.flex_space{
  justify-content: space-between;
}
view.flex_center{
  justify-content: center;
}
view.flex_shrink{
  flex-shrink: 0;
}
view.flex_end{
  align-items: flex-end;
}
.gray{
  color:#999;
}
.font13{
  font-size: 26rpx;
}
.white_bg {
  background-color: #fff;
}
@import "din/din.wxss";