/* pages/index/personMsg.wxss */
@import "../../icon/iconfont.wxss";
page{
  background: #f2f3f5;
}
.msgList{
  background: #fff;
  padding:0 30rpx;
  margin-top: 25rpx;
}
.list{
  line-height: 90rpx;
  border-bottom:1rpx solid #efefef;
}
.list:last-child{
  border-bottom: none;
}
.listName{
  font-size: 34rpx;
  color:#333;
}
.listContent {
  margin-top:-5rpx;
  margin-left: 80rpx;
}
.listContent input{
  font-size: 34rpx;
  color: #333;
  width:400rpx;
  margin-top: 26rpx
}
.picker{
  font-size: 34rpx;
  color:#333;
  margin-top: 7rpx;
  width:400rpx;
}
.phone{
  font-size: 34rpx;
  line-height: 98rpx;
  color: #333;
}
.getPhone{
  line-height: 98rpx;
  background: #fff;
  color:#999;
  font-size: 34rpx;
  padding: 0
}
.getPhone::after{
  border: none;
}