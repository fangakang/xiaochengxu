    <view class="iconfont icon-weibiaoti1"></view>
    <view>首页</view>
  </view>
  <view class="pageTo {{pageNum==4?'active':''}}" bindtap='privilege'>
    <view class="iconfont icon-huiyuan2"></view>
    <view>会员福利</view>
  </view>
  <view class="pageTo {{pageNum==2?'active':''}}" bindtap='search'>
    <view class="iconfont icon-sousuo-tianchong"></view>
