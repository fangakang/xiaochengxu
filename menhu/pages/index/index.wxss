  line-height:1;
  border-radius: 0;
}
.advertiseOne{
  margin-top: 25rpx;
}
.advertiseOne image{
  width:750rpx;
  height:450rpx;
}
.memberPrice{
  width: 80rpx;
  height:35rpx;
  margin-left: 25rpx;
}
.saleNum{
  position: absolute;
  color: #999;
  font-size: 24rpx;
  right:0;
  bottom:37rpx;
}
.notMember{
  margin-top: 65rpx;
}
.isMember{
